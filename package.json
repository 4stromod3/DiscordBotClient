{
	"name": "discord-bot-client",
	"version": "1.1.2",
	"description": "Discord Bot Client - login into discord with a bot token",
	"main": "electron.js",
	"dependencies": {
		"axios": "1.1",
		"discord-protos": "^1.0.4",
		"electron-log": "^4.4.8",
		"express": "^4.18.1",
		"node-fetch": "^2.6.7",
		"request": "^2.88.2",
		"ws": "^8.12.1"
	},
	"devDependencies": {
		"electron": "^15.5.7",
		"electron-builder": "^23.6.0"
	},
	"build": {
		"appId": "DiscordBotClient",
		"productName": "DiscordBotClient",
		"copyright": "Copyright Â© 2023 Elysia#1782",
		"mac": {
			"category": "public.app-category.social-networking",
			"target": [
				"dmg"
			],
			"icon": "DiscordBotClient.png"
		},
		"linux": {
			"target": [
				"AppImage"
			],
			"icon": "DiscordBotClient.png"
		},
		"win": {
			"target": [
				"portable",
				{
					"target": "nsis",
					"arch": [
						"x64",
						"ia32"
					]
				}
			],
			"icon": "DiscordBotClient.png"
		}
	},
	"scripts": {
		"start": "echo \"Error: no start script specified\" && exit 1",
		"mac": "electron-builder -m",
		"win": "electron-builder -w",
		"linux": "electron-builder -l"
	},
	"author": {
		"name": "aiko-chan-ai",
		"email": "DiscordBotClient@aiko.com"
	},
	"repository": {
		"url": "https://github.com/aiko-chan-ai/DiscordBotClient/"
	},
	"license": "ISC",
	"productName": "Discord Bot Client"
}
