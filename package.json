{
	"name": "discord-bot-client",
	"version": "1.0.0",
	"description": "Discord Bot Client - login into discord with a bot token",
	"main": "electron.js",
	"dependencies": {
		"axios": "1.1",
		"express": "^4.18.1",
		"node-fetch": "^2.6.7",
		"request": "^2.88.2"
	},
	"appId": "com.electron.discord-bot-client",
	"devDependencies": {
		"electron": "^15.5.7",
		"electron-builder": "^23.6.0"
	},
	"build": {
		"mac": {
			"category": "public.app-category.social-networking",
			"target": [
				"dmg",
				"zip"
			],
			"icon": "DiscordBotClient.png"
		},
		"linux": {
			"target": [
				"AppImage",
				"deb",
				"rpm",
				"freebsd",
				"pacman"
			],
			"icon": "DiscordBotClient.png"
		},
		"win": {
			"target": [
				{
					"target": "nsis",
					"arch": [
						"x64",
						"ia32"
					]
				}
			],
			"icon": "DiscordBotClient.png"
		}
	},
	"scripts": {
		"start": "electron electron.js",
		"pack:mac": "electron-builder --dir -m",
		"dist:mac": "electron-builder -m",
		"pack:win": "electron-builder --dir -w",
		"dist:win": "electron-builder -w",
		"pack:linux": "electron-builder --dir -l",
		"dist:linux": "electron-builder -l"
	},
	"author": {
		"name": "aiko-chan-ai",
		"email": "DiscordBotClient@aiko.com"
	},
	"repository": {
		"url": "https://github.com/aiko-chan-ai/DiscordBotClient/"
	},
	"license": "ISC",
	"productName": "Discord Bot Client"
}